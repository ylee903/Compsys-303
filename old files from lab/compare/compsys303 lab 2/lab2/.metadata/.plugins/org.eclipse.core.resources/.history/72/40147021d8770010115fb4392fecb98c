#include <system.h>
#include <altera_avalon_pio_regs.h>
#include <stdio.h>
#include "cs303lab2.h"
int main()
{
// Create the struct
TickData data;
// Initialise
reset(&data);
while(1)
{
// Fetch button inputs
// A is Key 2, B is Key 1, R is Key 0
// Remember that keys are active low
// Do a tick!
tick(&data);
// Output O to Red LED
}
return 0;
}
